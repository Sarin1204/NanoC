FUNCSTART swap
OPENSCOPE
DECLI arg1
DECLI arg2
DECLI temp
PUSH 0
STORE temp
PUSH arg1
STORE temp
PUSH arg2
STORE arg1
PUSH temp
STORE arg2
PUSH arg2
DECLI theReturnVariable
STORE theReturnVariable
RET theReturnVariable
ENDSCOPE
FUNCEND swap
FUNCSTART start
OPENSCOPE
DECLI a
PUSH 0
STORE a
DECLI arg1
PUSH 1
STORE arg1
DECLI arg2
PUSH 2
STORE arg2
CALL swap arg1 arg2 
STORE a
PRINTLN 'arg2%==%' a
ENDSCOPE
FUNCEND start
CALL start
EOF
